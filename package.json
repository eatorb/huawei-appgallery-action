{
  "name": "huawei-appgallery-action",
  "version": "1.0.0",
  "description": "GitHub Action to deploy apps to Huawei AppGallery",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build lib/index.js --source-map --license licenses.txt",
    "all": "npm run build && npm run package",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "github",
    "action",
    "huawei",
    "appgallery",
    "android"
  ],
  "author": "eatorb",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.1",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vercel/ncc": "^0.38.0",
    "typescript": "^5.3.3"
  }
}